<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.model.test_participateConDAO"> 
	
	<select id="selectOneBoardnumber" parameterType="int" resultType="String">
		select CAT_NAME from tbl_share where BOARD_SEQ=#{num}
	</select>
	
	<insert  id="insertBuyBoardMember" parameterType="com.smhrd.model.test_participatelistCon">
		insert into tbl_buy_applicant values (tbl_buy_applicant_seq.nextval,#{MEM_ID},sysdate,'N','모집중',null,null,#{CAT_NAME},#{BOARD_SEQ})	
	</insert>
	
	<insert  id="insertArbeitBoardMember" parameterType="com.smhrd.model.test_participatelistCon">
		insert into tbl_buy_applicant values (tbl_Arbeit_applicant_seq.nextval,#{MEM_ID},sysdate,'N','모집중',null,null,null,#{CAT_NAME},#{BOARD_SEQ})	
	</insert>
	
	<insert  id="insertFreeBoardMember" parameterType="com.smhrd.model.test_participatelistCon">
		insert into tbl_buy_applicant values (tbl_Free_applicant_seq.nextval,sysdate,#{MEM_ID},'N','모집중',#{CAT_NAME},#{BOARD_SEQ})	
	</insert>
	
	<insert  id="insertAccountBoardMember" parameterType="com.smhrd.model.test_participatelistCon">
		insert into tbl_buy_applicant values (tbl_Account_applicant_seq.nextval,#{MEM_ID},sysdate,'N','모집중',null,null,null,#{CAT_NAME},#{BOARD_SEQ})	
	</insert>
	
	</mapper>