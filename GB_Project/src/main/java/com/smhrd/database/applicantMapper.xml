<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.model.tbl_applicantDAO"> 

<insert  id="insertBuyBoardMember" parameterType="com.smhrd.model.tbl_buy_applicant">
		insert into tbl_buy_applicant values (tbl_buy_applicant_seq.nextval,#{writer},sysdate,'진행','진행',#{buylink},#{buypay},'B',0)	
	</insert>
	
	<insert  id="insertArbeitBoardMember" parameterType="com.smhrd.model.tbl_Arbeit_applicant">
		insert into tbl_buy_applicant values (tbl_Arbeit_applicant_seq.nextval,#{writer},sysdate,'진행','진행',#{ARBEIT_TIME},#{ARBEIT_SITE},#{ARBEIT_PAY},'A')	
	</insert>
	
	<insert  id="insertFreeBoardMember" parameterType="com.smhrd.model.tbl_Free_applicant">
		insert into tbl_buy_applicant values (tbl_Free_applicant_seq.nextval,sysdate,#{writer},'진행','진행','F')	
	</insert>
	
	<insert  id="insertAccountBoardMember" parameterType="com.smhrd.model.tbl_Account_applicant">
		insert into tbl_buy_applicant values (tbl_Account_applicant_seq.nextval,#{writer},sysdate,'진행','진행',#{ID_LINK},#{ID_TIME},#{ID_PAY},'I')	
	</insert>

</mapper>